{
  "name": "easy-mysqlx",
  "version": "2.0.1",
  "description": "fast CURD api tools based node-mysql2",
  "main": "lib/index.js",
  "types": "lib/typing/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install && rm -rf lib && npm run build",
    "lint-staged": "lint-staged",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lancernix/easy-mysqlx.git"
  },
  "keywords": [
    "node",
    "mysql"
  ],
  "author": "lancernix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lancernix/easy-mysqlx/issues"
  },
  "homepage": "https://github.com/Lancernix/easy-mysqlx#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "promise-mysql": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.2"
  }
}
